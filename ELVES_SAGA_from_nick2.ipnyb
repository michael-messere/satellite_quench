    "    angles = np.asarray(angles)\n",
    "    n = len(angles)\n",
    "\n",
    "    theta_grid = np.sort(angles)\n",
    "    cdf_boot = np.zeros((n_boot, len(theta_grid)))\n",
    "\n",
    "    for i in range(n_boot):\n",
    "        sample = np.random.choice(angles, size=n, replace=True)\n",
    "        sample_sorted = np.sort(sample)\n",
    "        cdf_boot[i] = np.searchsorted(sample_sorted, theta_grid, side=\"right\") / n\n",
    "\n",
    "    cdf_med = np.median(cdf_boot, axis=0)\n",
    "    cdf_lo  = np.percentile(cdf_boot, 16, axis=0)\n",
    "    cdf_hi  = np.percentile(cdf_boot, 84, axis=0)\n",
    "\n",
    "    return theta_grid, cdf_med, cdf_lo, cdf_hi"
   ]
