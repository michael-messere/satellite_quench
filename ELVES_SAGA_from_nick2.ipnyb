    "# convert to 0-90\n",
    "\n",
    "def map_to_0_90(angles):\n",
    "\n",
    "    angles = np.mod(angles, 360)\n",
    "    return np.where(angles <= 90, angles,  \n",
    "           np.where(angles <= 180, 180 - angles,  \n",
    "           np.where(angles <= 270, angles - 180,  \n",
    "                    360 - angles))) \n",
    "\n",
