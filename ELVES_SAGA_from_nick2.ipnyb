    "customSimbad.reset_votable_fields()\n",
    "# Add degree-valued RA/DEC + host PA\n",
    "customSimbad.add_votable_fields('ra', 'dec', 'galdim_angle','galdim_majaxis','galdim_minaxis')\n"
   ]
