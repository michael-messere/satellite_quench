    "    elves_90_correct.append(elves_90[i][0])\n",
    "x = np.array(elves_90_correct)\n",
    "y = np.array(elves_quenched)\n",
    "\n",
    "bin_centers, fq_mean_elves, fq_std_elves = bootstrap_90_observational_binned(x, y, N=10000)"
   ]
