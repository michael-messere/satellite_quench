    "def bootstrap_delta_cdf(angles_q, angles_sf, n_boot=5000):\n",
    "    n_q, n_sf = len(angles_q), len(angles_sf)\n",
    "    theta_grid = np.linspace(0, 90, 200)\n",
    "\n",
    "    delta_boot = np.zeros((n_boot, len(theta_grid)))\n",
    "\n",
    "    for i in range(n_boot):\n",
    "        q_bs  = np.random.choice(angles_q,  n_q,  replace=True)\n",
    "        sf_bs = np.random.choice(angles_sf, n_sf, replace=True)\n",
    "\n",
    "        cdf_q  = np.array([np.mean(q_bs  <= t) for t in theta_grid])\n",
    "        cdf_sf = np.array([np.mean(sf_bs <= t) for t in theta_grid])\n",
    "\n",
    "        delta_boot[i] = cdf_q - cdf_sf\n",
    "\n",
    "    delta_med = np.median(delta_boot, axis=0)\n",
    "    delta_lo  = np.percentile(delta_boot, 16, axis=0)\n",
    "    delta_hi  = np.percentile(delta_boot, 84, axis=0)\n",
    "\n",
    "    return theta_grid, delta_med, delta_lo, delta_hi\n"
   ]
