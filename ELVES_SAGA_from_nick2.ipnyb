    "def alignment_significance(angles, n_boot=10000):\n",
    "    angles = np.asarray(angles)\n",
    "\n",
    "    # empirical CDF\n",
    "    theta_sorted = np.sort(angles)\n",
    "    cdf = np.arange(1, len(theta_sorted) + 1) / len(theta_sorted)\n",
    "\n",
    "    # isotropic CDF (2D projected)\n",
    "    iso_cdf = theta_sorted / 90.0\n",
    "\n",
    "    # observed statistic\n",
    "    S_obs = np.max(np.abs(cdf - iso_cdf))\n",
    "\n",
    "    # bootstrap under isotropy\n",
    "    S_boot = np.zeros(n_boot)\n",
    "    n = len(angles)\n",
    "\n",
    "    for i in range(n_boot):\n",
    "        theta_iso = np.random.uniform(0, 90, n)\n",
    "        theta_iso.sort()\n",
    "        cdf_iso = np.arange(1, n + 1) / n\n",
    "        S_boot[i] = np.max(np.abs(cdf_iso - theta_iso / 90.0))\n",
    "\n",
    "    p_value = np.mean(S_boot >= S_obs)\n",
    "\n",
    "    return S_obs, p_value\n",
    "\n",
    "S_q, p_q   = alignment_significance(angles_q)\n",
    "S_sf, p_sf = alignment_significance(angles_sf)\n",
    "\n",
    "print(f\"Q:  S = {S_q:.3f}, p = {p_q:.3e}\")\n",
    "print(f\"SF: S = {S_sf:.3f}, p = {p_sf:.3e}\")"
   ]
